    "import pandas as pd\n",
    "\n",
    "rf = pd.read_csv('csv/rf_list.csv')\n",
    "rf.drop(columns='Unnamed: 0', inplace=True)\n",
    "rf.set_index(keys='일자', drop=True, inplace=True)\n",
    "rf.index = pd.to_datetime(rf.index)\n",
    "\n",
    "rf = rf.resample('M').first()\n",
    "\n",
    "rf"
